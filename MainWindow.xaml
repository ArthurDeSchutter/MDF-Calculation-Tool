        <Button x:Name="open_suppliers" Content="open_suppliers" HorizontalAlignment="Left" Margin="24,22,0,0" VerticalAlignment="Top" Click="open_suppliers_Click"/>
        <Button Content="Open PaymentMethods" HorizontalAlignment="Left" Margin="155,22,0,0" VerticalAlignment="Top" Click="Button_Click"/>
    </Grid>
</Window>
